stoptime=5.
rtf=1.
[zmq_cont]
  binary=zmq_cont_adapter
  np=1
  music_timestep=1.01
  message_type=GymObservation
  zmq_topic=
  zmq_addr=tcp://localhost:5556
[threshold]
  binary=threshold_adapter
  np=1
  music_timestep=1.01
  is_heaviside=false
[cont_zmq]
  binary=cont_zmq_adapter
  np=1
  music_timestep=1.01
  message_type=GymCommand
  min=-1.
  max=1.
  zmq_topic=
  zmq_addr=tcp://*:5557
zmq_cont.out->threshold.in[1]
threshold.out->cont_zmq.in[1]
